    "cors": "^2.8.5",
    "@aws-sdk/client-ec2": "^3.478.0",
    "@aws-sdk/client-sts": "^3.478.0",
    "uuid": "^9.0.1",
    "sqlite3": "^5.1.6",
    "dotenv": "^16.3.1",
    "socket.io": "^4.7.4",
    "socket.io-client": "^4.7.4",
